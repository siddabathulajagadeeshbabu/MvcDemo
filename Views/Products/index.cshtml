@model List<MvcDemo.Models.Product>    <h1> Prodcut Lis </h1>    <table class="table">        <thead>            <tr>                <th>Id</th>                <th>Name</th>                <th>Price</th>            </tr>        </thead>        <tbody>            @foreach (var product in Model)            {            <tr>                <td>@product.Id</td>                <td>@product.Name</td>                <td>@product.Price.ToString("C")</td>            </tr>            }        </tbody>    </table>